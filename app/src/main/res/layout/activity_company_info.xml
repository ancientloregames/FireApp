		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView8"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		app:layout_constraintTop_toTopOf="@+id/row_06"
		app:layout_constraintEnd_toStartOf="@+id/column_02"
		/>
	<TextView
		android:text="+7(4012)92-29-72"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView10"
		android:textIsSelectable="true"
		app:layout_constraintTop_toBottomOf="@+id/textView3"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		app:layout_constraintEnd_toStartOf="@+id/column_02"
		/>
	<TextView
		android:text="+7(4012)92-29-73"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView11"
		android:textIsSelectable="true"
		app:layout_constraintTop_toBottomOf="@+id/textView5"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		app:layout_constraintEnd_toStartOf="@+id/column_02"
		/>
	<TextView
		android:text="+7(4012)79-21-51"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView12"
		android:textIsSelectable="true"
		app:layout_constraintTop_toBottomOf="@+id/textView6"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		app:layout_constraintEnd_toStartOf="@+id/column_02"
		/>
	<TextView
		android:text="Электронные адреса для связи:"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/textView"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		app:layout_constraintTop_toTopOf="@+id/row_07"
		/>
	<TextView
		android:text="aa@AlitaParts.ru"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView9"
		android:textIsSelectable="true"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		app:layout_constraintTop_toBottomOf="@+id/textView"
		/>
	<TextView
		android:text="za@AlitaParts.ru"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/textView2"
		android:textIsSelectable="true"
		app:layout_constraintTop_toBottomOf="@+id/textView9"
		app:layout_constraintStart_toStartOf="@+id/column_03"
		/>
</android.support.constraint.ConstraintLayout>