<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:fitsSystemWindows="true"
	android:background="@color/colorAccent"
	tools:context=".CompanyInfoActivity"
	>
	<include
		layout="@layout/topbar"
		android:id="@+id/include"
		/>
	<ImageView
		android:layout_width="0dp"
		android:layout_height="0dp"
		app:srcCompat="@android:color/white"
		android:id="@+id/imageView3"
		app:layout_constraintTop_toBottomOf="@+id/include"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		android:contentDescription="@null"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintVertical_bias="1.0"
		/>
	<!-- Guidelines -->
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/column_01"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.02"
		app:layout_constraintStart_toStartOf="parent"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/column_02"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.58"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/column_03"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.06"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/column_04"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.74"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_01"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="63dp"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_02"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="97dp"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_03"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="89dp"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_04"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="127dp"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_05"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="165dp"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_06"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="210dp"
		/>
	<android.support.constraint.Guideline
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/row_07"
		android:orientation="horizontal"
		app:layout_constraintGuide_begin="270dp"
		/>
	<!-- Content -->
	<TextView
		android:text="г. Калининград"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/title01View"
		app:layout_constraintTop_toTopOf="@+id/row_01"
		app:layout_constraintStart_toStartOf="@+id/column_01"
		app:layout_constraintEnd_toStartOf="@+id/column_02"
		/>
	<TextView
		android:text="Время работы"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/title02View"
		app:layout_constraintTop_toTopOf="@+id/row_01"
		app:layout_constraintStart_toStartOf="@+id/column_02"
		app:layout_constraintEnd_toEndOf="parent"
		/>
	<TextView
		android:text="1. ул.Карла Маркса, д.56"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView3"
